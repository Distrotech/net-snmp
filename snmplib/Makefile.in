#
# Makefile for snmplib
#
SHELL = /bin/sh
srcdir = @srcdir@
VPATH = @srcdir@
@SET_MAKE@
prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@/ucd-snmp

TARG=	libsnmp.a
CSRCS=	snmp_client.c mib.c parse.c snmp_api.c snmp.c snmp_auth.c asn1.c md5.c party.c context.c acl.c view.c party_parse.c context_parse.c acl_parse.c system.c
OBJS=	snmp_client.o mib.o parse.o snmp_api.o snmp.o snmp_auth.o asn1.o md5.o party.o context.o acl.o view.o party_parse.o context_parse.o acl_parse.o system.o

INSTALLHEADERS=asn1.h snmp_api.h snmp_client.h snmp_impl.h snmp.h parse.h mib.h

CFLAGS=@CFLAGS@
CPPFLAGS=@CPPFLAGS@ -I.. -I$(srcdir)/..
LDFLAGS=@LDFLAGS@
INSTALL = @INSTALL@

CC = @CC@ $(CPPFLAGS)
RANLIB=@RANLIB@

all: $(TARG)

libsnmp.a:    ${OBJS}
	ar r libsnmp.a ${OBJS}
	$(RANLIB) libsnmp.a

installdirs:
	@$(SHELL) $(srcdir)/../mkinstalldirs $(libdir) $(includedir)

install:	$(TARG) installdirs
	@for i in $(TARG) ; do $(INSTALL) $$i $(libdir) ; echo "install:  installed $$i in $(libdir)" ; done
	@for i in $(INSTALLHEADERS) ; do $(INSTALL) $$i $(includedir) ; echo "install:  installed $$i in $(includedir)" ; done

../lib/libsnmp.a:	libsnmp.a
	cp libsnmp.a ../lib
	@RANLIB@ ../lib/libsnmp.a

parse:  mib.o parse.c
	$(CC) $(CFLAGS) -DTEST parse.c mib.o -o parse

lint:
	lint -nhx $(CSRCS)

clean:
	rm -f ${OBJS} ${TARG}
	rm -f ../lib/${TARG}

depend: 
	makedepend $(CPPFLAGS) $(srcdir)/*.c

nosysdepend:
	makedepend $(CPPFLAGS) *.c
	perl -n -i.bak -e ' if (/^# DO NOT DELETE THIS LINE/) {$$doit=1;} if ($$doit == 1) {s#/usr/include/[^\s]+##g ; print if (! /^[\w]+\.o:\s*$$/);} else {print;}' Makefile

makefileindepend: nosysdepend
	perl -e ' rename("Makefile.in","Makefile.in.bak"); open(F,"Makefile.in.bak"); open(G,"Makefile"); open(O,">Makefile.in"); $$_ = <F>; while(!/^# DO NOT DELETE THIS LINE/) { print O $$_; $$_ = <F>; } print O $$_; $$_ = <G>; while(!/^# DO NOT DELETE THIS LINE/) { $$_ = <G>; } while (<G>) {print O $$_;} '

